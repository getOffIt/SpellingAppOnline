app_identifier(ENV['APPFILE_APP_IDENTIFIER']) # The bundle identifier of your app
apple_id(ENV['APPFILE_APPLE_ID']) # Your Apple Developer Portal username
itc_team_id(ENV['APPFILE_TEAM_ID']) # App Store Connect Team ID
team_id(ENV['APPFILE_TEAM_ID']) # Developer Portal Team ID

