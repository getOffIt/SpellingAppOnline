git_url("git@github.com:getOffIt/match.git")

storage_mode("git")
app_identifier(ENV['APPFILE_APP_IDENTIFIER'])
type("appstore") # The default type, can be: appstore, adhoc, enterprise or development
# if environmenet variable is specified, used this otherwise default value is appstore.
match(type: ENV["MATCH_TYPE"] || "appstore", app_identifier: ENV['APP_IDENTIFIER'])

